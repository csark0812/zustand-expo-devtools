{
  "name": "zustand-expo-devtools",
  "version": "1.0.0",
  "description": "DevTools plugin for debugging Zustand stores in Expo applications",
  "main": "plugin/build/index.js",
  "types": "plugin/build/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "cd plugin && npm run build",
    "prepare": "cd plugin && npm run prepare",
    "prepublishOnly": "cd plugin && npm run prepublishOnly",
    "dev": "cd example && npx expo start",
    "example:install": "cd example && npm install",
    "plugin:install": "cd plugin && npm install",
    "install:all": "npm run plugin:install && npm run example:install",
    "clean": "cd plugin && npm run clean",
    "lint": "cd plugin && npx eslint src --ext .ts,.tsx",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": [
    "zustand",
    "expo",
    "devtools",
    "debugging",
    "state-management",
    "react-native",
    "expo-plugin"
  ],
  "files": ["plugin/build", "plugin/expo-module.config.json", "README.md", "LICENSE"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YOUR_USERNAME/zustand-expo-devtools.git"
  },
  "bugs": {
    "url": "https://github.com/YOUR_USERNAME/zustand-expo-devtools/issues"
  },
  "homepage": "https://github.com/YOUR_USERNAME/zustand-expo-devtools#readme",
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "dependencies": {
    "zustand": "^5.0.5"
  },
  "peerDependencies": {
    "expo": ">=50.0.0",
    "react": ">=18.0.0",
    "react-native": ">=0.70.0"
  },
  "devDependencies": {
    "@types/react": "~19.0.10",
    "expo": "~53.0.3",
    "expo-module-scripts": "^4.1.7",
    "react": "19.0.0",
    "typescript": "~5.8.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
